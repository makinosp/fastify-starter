{
  "name": "fastify-starter",
  "version": "1.0.0",
  "description": "fastify-starter",
  "homepage": "https://github.com/makinosp/fastify-starter#readme",
  "bugs": {
    "url": "https://github.com/makinosp/fastify-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makinosp/fastify-starter.git"
  },
  "license": "BSD-3-Clause",
  "author": "Maki Makino",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "run-p ts:transpile:*",
    "dev": "tsx watch -r tsconfig-paths/register src/app.ts",
    "docs:specs:generate": "tsx -r tsconfig-paths/register scripts/generate-api-specs.ts",
    "inspect": "run-p typecheck lint",
    "lint": "eslint --concurrency auto --cache .",
    "lint:fix": "eslint --concurrency auto --cache --fix .",
    "model:generate": "prisma generate",
    "start": "node ./dist/src/app.js",
    "ts:typecheck": "tsc",
    "ts:transpile:src": "swc ./src -d ./dist",
    "ts:transpile:orm": "swc ./prisma/generated -d ./dist --copy-files",
    "typecheck": "run-s model:generate ts:typecheck",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.5",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.11",
    "@types/node": "^25.1.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^7.3.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/postgres": "^6.0.2",
    "@joggr/fastify-prisma": "^7.0.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@swc/helpers": "^0.5.18",
    "fastify-type-provider-zod": "^6.1.0",
    "pg": "^8.17.2",
    "zod": "^4.3.6"
  }
}
